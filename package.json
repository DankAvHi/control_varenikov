{
     "name": "control_varenikov",
     "version": "1.0.0",
     "description": "admin panel for vareniki",
     "main": "index.js",
     "scripts": {
          "build": "npm run build-client && npm run build-server",
          "build-windows": "npm run build-client && npm run build-server-windows",
          "start-dev-windows": "SET NODE_ENV=development&& npx nodemon index.ts",
          "start-windows": "SET NODE_ENV=production&& npm run build-windows && node dist/index.js",
          "start": "NODE_ENV=production&& npm run build && node dist/index.js",
          "build-server": "npx del-cli ./dist && webpack --mode production",
          "build-server-windows": "npx del-cli ./dist && webpack --config webpack.config.windows.js --mode production",
          "build-client": "cd client && npm run build && cd ../"
     },
     "author": "",
     "license": "ISC",
     "dependencies": {
          "@prisma/client": "^4.0.0",
          "bcrypt": "^5.0.1",
          "cookie": "^0.5.0",
          "cookie-parser": "^1.4.6",
          "cors": "^2.8.5",
          "dotenv": "^16.0.1",
          "express": "^4.18.1",
          "express-session": "^1.17.3",
          "jsonwebtoken": "^8.5.1",
          "multer": "^1.4.5-lts.1",
          "passport": "^0.6.0",
          "passport-jwt": "^4.0.0",
          "passport-local": "^1.0.0"
     },
     "devDependencies": {
          "@types/bcrypt": "^5.0.0",
          "@types/cookie-parser": "^1.4.3",
          "@types/cors": "^2.8.12",
          "@types/express": "^4.17.13",
          "@types/express-session": "^1.17.5",
          "@types/jsonwebtoken": "^8.5.8",
          "@types/multer": "^1.4.7",
          "@types/node": "^18.0.4",
          "@types/passport": "^1.0.9",
          "@types/passport-jwt": "^3.0.6",
          "@types/passport-local": "^1.0.34",
          "copy-webpack-plugin": "^11.0.0",
          "cpx": "^1.5.0",
          "nodemon": "^2.0.19",
          "prisma": "^4.0.0",
          "ts-loader": "^9.3.1",
          "ts-node": "^10.9.1",
          "typescript": "^4.7.4",
          "webpack": "^5.73.0",
          "webpack-cli": "^4.10.0",
          "webpack-node-externals": "^3.0.0",
          "webpack-shell-plugin": "^0.5.0"
     }
}
